#include <stdio.h>
#include <sys/types.h>
#include <sys/stat.h>
#include <fcntl.h>
#include <unistd.h>
#include <stdlib.h>

int main(int argc, char** argv){
  int A = argv[1];
  int f = argv[2];
  int n = argv[3];
  for (int i = 0; i < n; i++){
    char x = A*sin(2*M_PI*f*i);
    printf("%d", x);
  }
}
